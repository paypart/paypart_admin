                        </li>
                        <li class="side-nav-links" id="dash" style="display:@Model.authdata.showDash">
                            <a class="active"><i class="fa fa-dashboard fa-fw"></i> Dashboard</a>
                        </li>
                        <li style="display:@Model.authdata.showAdmin">
                            <a href="#"><i class="fa fa-user fa-fw"></i> Administrators<span class="fa arrow"></span></a>
                            <ul class="nav collapse nav-second-level">
                                <li class="side-nav-links" id="new-admin">
                                    <a>New</a>
                                </li>
                                <li class="side-nav-links" id="manage-admin">
                                    <a>Manage</a>
                                </li>
                            </ul>
                        </li>
                        <li style="display:@Model.authdata.showOrg">
                            <a href="#"><i class="fa fa-building fa-fw"></i> Biller Categories<span class="fa arrow"></span></a>
                            <ul class="nav nav-second-level collapse">
                                <li class="side-nav-links" id="new-biller-category">
                                    <a>New</a>
                                </li>
                                <li class="side-nav-links" id="manage-biller-category">
                                    <a>Manage</a>
                                </li>
                            </ul>
                        </li>
                        <li style="display:@Model.authdata.showOrg">
                            <a href="#"><i class="fa fa-building fa-fw"></i> Billers<span class="fa arrow"></span></a>
                            <ul class="nav nav-second-level collapse">
                                <li class="side-nav-links" id="new-biller">
                                    <a>New</a>
                                </li>
                                <li class="side-nav-links" id="manage-biller">
                                    <a>Manage</a>
                                </li>
                            </ul>
                        </li>
                        <li style="display:@Model.authdata.showClass">
                            <a href="#"><i class="fa fa-university fa-fw"></i> Service Categories<span class="fa arrow"></span></a>
                            <ul class="nav nav-second-level collapse">
                                <li class="side-nav-links" id="new-service-category">
                                    <a>New</a>
                                </li>
                                <li class="side-nav-links" id="manage-service-category">
                                    <a>Manage</a>
                                </li>
                            </ul>
                        </li>
                        <li style="display:@Model.authdata.showTerm">
                            <a href="#"><i class="fa fa-mortar-board fa-fw"></i> Services<span class="fa arrow"></span></a>
                            <ul class="nav nav-second-level collapse">
                                <li class="side-nav-links" id="new-service">
                                    <a>New</a>
                                </li>
                                <li class="side-nav-links" id="manage-service">
                                    <a>Manage</a>
                                </li>
                            </ul>
                        </li>
                        <li style="display:none">
                            <a href="#"><i class="fa fa-money fa-fw"></i> Products<span class="fa arrow"></span></a>
                            <ul class="nav nav-second-level collapse">
                                <li class="side-nav-links" id="new-product">
                                    <a>New</a>
                                </li>
                                <li class="side-nav-links" id="manage-product">
                                    <a>Manage</a>
                                </li>
                            </ul>
                        </li>
                        <li style="display:@Model.authdata.showFees">
                            <a href="#"><i class="fa fa-money fa-fw"></i> Items<span class="fa arrow"></span></a>
                            <ul class="nav nav-second-level collapse">
                                <li class="side-nav-links" id="new-service-item">
                                    <a>New</a>
                                </li>
                                <li class="side-nav-links" id="manage-service-item">
                                    <a>Manage</a>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
        <div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header lbltag">@Model.authdata.pageheader</h1>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <div id="page-body">
                @if (Model.authdata.activepage == "AddAdmin")
                {
                    @Html.Partial("/Views/Admin/_Add_Admin.cshtml", Model.user)
                }
                else if (Model.authdata.activepage == "AddBillerCategory")
                {
                    @Html.Partial("/Views/BillerCategory/_Add_Biller_Category.cshtml", Model.billercategory)
                }
                else if (Model.authdata.activepage == "AddBiller")
                {
                    @Html.Partial("/Views/Biller/_Add_Biller.cshtml", Model.biller)
                }
                else if (Model.authdata.activepage == "AddServiceCategory")
                {
                    @Html.Partial("/Views/ServiceCategory/_Add_Service_Category.cshtml", Model.servicecategory)
                }
                else if (Model.authdata.activepage == "AddService")
                {
                    @Html.Partial("/Views/Service/_Add_Service.cshtml", Model.service)
                }
                else if (Model.authdata.activepage == "Dashboard")
                {
                    @Html.Partial("_Dashboard")
                }
                else
                {
                    @Html.Partial("_Dashboard")
                }
            </div>
            @*<div id="page-body">
                    @Html.Partial("/Views/Admin/_Add_Admin.cshtml", new paypart_admin.Models.User())
                </div>*@

            <!-- /.panel-body -->
            <div class="panel-footer">

            </div>
            <!-- /.panel-footer -->
        </div>
        <!-- /.panel .chat-panel -->
    </div>
    <script src="~/vendor/jquery/jquery.min.js"></script>
    <script type="text/javascript">
        $(function () {
            var email = $("#authdata_email").val();
            sessionStorage.setItem('_email', email);

            var token = $("#authdata_auth").val();
            sessionStorage.setItem('_token', token);
            //alert(sessionStorage.getItem('_token'))
        });
    </script>
