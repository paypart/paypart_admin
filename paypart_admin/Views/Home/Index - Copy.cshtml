@model paypart_admin.Models.authData
@{
    ViewData["Title"] = "Home";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    @*<meta http-equiv="X-UA-Compatible" content="IE=edge">*@
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>Paypart Admin - Home</title>
    <link href="~/css/jquery-ui.css" rel="stylesheet" />
    <!-- Bootstrap Core CSS -->
    @*<link href="~/css/bootstrap.min.css" rel="stylesheet">*@
    <!-- MetisMenu CSS -->
    <link href="~/css/metisMenu.min.css" rel="stylesheet" />
    <!-- Timeline CSS -->
    <link href="~/css/startmin.css" rel="stylesheet" />
    <link href="~/css/typeahead.css" rel="stylesheet" />
    <link href="~/css/bootstrap-tagsinput.css" rel="stylesheet" />
    <!-- Custom CSS -->
    @*<link href="~/css/sb-admin-2.css" rel="stylesheet">*@
    <!-- Morris Charts CSS -->
    <link href="~/css/morris.css" rel="stylesheet" />
    @*<link href="~/css/site.css" rel="stylesheet">*@
    <!-- Custom Fonts -->
    <link href="~/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
    @*<link href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet" type="text/css" />*@
    <link href="https://cdn.datatables.net/1.10.13/css/dataTables.bootstrap.min.css" rel="stylesheet" type="text/css" />
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>
    <div id="wrapper">
        <div class="modal fade" id="subModal" role="dialog">
            <div class="modal-dialog" style="vertical-align:middle;text-align:center;top:40%">
                <span class="span-circle">
                    <i class="fa fa-spinner fa-spin fa-3x circle" style="color:#fff" aria-hidden="true"></i>
                    <strong style="color:white;">Submitting....</strong>
                    <span class="sr-only circle">Submitting...</span>
                </span>
            </div>
        </div>
        <div class="modal fade" id="sucModal" role="dialog">
            <div class="modal-dialog">
                <div class="modal-content11">
                    <div class="alert alert-success" id="success-alert">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <strong>Success </strong>
                        <label id="successlbl"></label>
                    </div>
                </div>
            </div>
        </div>
        <div class="modal fade" id="failModal" role="dialog">
            <div class="modal-dialog">
                <div class="alert alert-danger" id="danger-alert">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <strong>Failed </strong>
                    <label id="failurelbl"></label>
                </div>
            </div>
        </div>
        <div class="modal fade" id="warnModal" role="dialog">
            <div class="modal-dialog">
                <div class="alert alert-warning" id="warning-alert">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <strong>Caution </strong>
                    <label id="cautionlbl"></label>
                </div>
            </div>
        </div>
        <div class="modal fade" id="regFilesModal" role="dialog">
            <div class="modal-dialog">
                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 class="modal-title">Manage Attached Files</h4>
                    </div>
                    <div class="modal-body">
                        <table class="table" id="tblfiles">
                            <tr>
                                <th>File</th>
                                <th align="right" class="td-close-file">Remove</th>
                            </tr>
                        </table>

                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                    </div>
                </div>
            </div>
        </div>
        <div class="modal fade" id="editClassModal" role="dialog">
            <div class="modal-dialog">
                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 class="modal-title">Edit Class</h4>
                    </div>
                    <div class="modal-body">
                        <form role="form" id="form-edit-class" action="" name="form-edit-class">
                            <fieldset>
                                <div class="row" style="min-height:80px;">
                                    <div class="col-md-12">
                                        <div class="form-group">
                                            <label>Prefix</label>
                                            <input type="text" id="txtClassEditPrefix" class="form-control" name="txtClassEditPrefix" placeholder="Prefix: e.g Basic,Primary,JS" />
                                            <label id="lblClassEditPrefix"></label>
                                        </div>
                                    </div>
                                    <div class="col-md-12">
                                        <div class="form-group">
                                            <label>Class</label>
                                            <input type="number" id="txtClassEditNumber" class="form-control" name="txtClassEditNumber" placeholder="Class: e.g 1,2,4" />
                                            <label id="lblClassEditNumber"></label>
                                        </div>
                                    </div>
                                </div>
                                <div class="modal-footer">
                                    <button type="submit" id="btnEditClass" class="btn btn-primary">Submit</button>
                                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                                </div>
                            </fieldset>
                        </form>
                    </div>
                </div>
            </div>
        </div>
        <div class="modal fade" id="editTermModal" role="dialog">
            <div class="modal-dialog">
                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 class="modal-title">Edit Term</h4>
                    </div>
                    <div class="modal-body">
                        <form role="form" id="form-edit-term" action="" name="form-edit-term">
                            <fieldset>
                                <div class="row" style="min-height:80px;">
                                    <div class="col-md-12">
                                        <div class="form-group">
                                            <label>Term</label>
                                            <select id="ddlTermEditItem" class="form-control" name="ddlTermEditItem">
                                                <option value="0">Select Term</option>
                                                <option value="1">First</option>
                                                <option value="2">Second</option>
                                                <option value="3">Third</option>
                                                <option value="4">Fourth</option>
                                                <option value="5">Fifth</option>
                                                <option value="6">Sixth</option>
                                                <option value="7">Seventh</option>
                                                <option value="8">Eight</option>
                                            </select>
                                            <label id="lblTermEditItem"></label>
                                        </div>
                                    </div>
                                    <div class="col-md-12">
                                        <div class="form-group">
                                            <label>Suffix</label>
                                            <input type="text" id="txtTermEditSuffix" class="form-control" name="txtTermEditSuffix" placeholder="Suffix: e.g Term,Semester,Year" />
                                            <label id="lblTermEditSuffix"></label>
                                        </div>
                                    </div>
                                </div>
                                <div class="row" style="min-height:80px;">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label>Term Starts</label>
                                            <input type="text" id="txtTermEditStart" class="form-control" name="txtTermEditStart" />
                                            <label id="lblTermEditStart"></label>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label>Term Ends</label>
                                            <input type="text" id="txtTermEditEnds" class="form-control" name="txtTermEditEnds" />
                                            <label id="lblTermEditEnds"></label>
                                        </div>
                                    </div>
                                </div>
                                <div class="modal-footer">
                                    <button type="submit" id="btnEditTerm" class="btn btn-primary">Submit</button>
                                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                                </div>
                            </fieldset>
                        </form>
                    </div>
                </div>
            </div>
        </div>
        <div class="modal fade" id="deleteModal" role="dialog">
            <div class="modal-dialog">
                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 class="modal-title"><label id="lblmoddeleteheader"></label></h4>
                    </div>
                    <div class="modal-body">
                        <div class="row">
                            <form id="mod12-form" action="" name="mod12-form">
                                <div class="col-md-12" style="height:50px;">
                                    <div class="form-group">
                                        <label id="lblmoddelete"></label>
                                        <input type="hidden" id="hdndeletedependency" />
                                        <input type="hidden" id="hdndeletefromcontrol" />
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-custom" id="btnmoddelete" data-dismiss="modal">Delete</button>
                        <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
                    </div>
                </div>
            </div>
        </div>
        <!-- Navigation -->
        <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
            <div class="navbar-header">
                <a class="navbar-brand" href="index.html">Payzone</a>
            </div>
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <ul class="nav navbar-nav navbar-left navbar-top-links">
                <li><a href="#"><i class="fa fa-home fa-fw"></i> Website</a></li>
            </ul>
            <ul class="nav navbar-right navbar-top-links">
                <li class="dropdown">
                    <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                        <i class="fa fa-user fa-fw"></i> <label id="lblUname">@Model.username</label> <b class="caret"></b>
                    </a>
                    <ul class="dropdown-menu dropdown-user">
                        @*<li>
                                <a href="#"><i class="fa fa-user fa-fw"></i> User Profile</a>
                            </li>
                            <li>
                                <a href="#"><i class="fa fa-gear fa-fw"></i> Settings</a>
                            </li>*@
                        <li class="divider"></li>
                        <li style="cursor:pointer;">
                            <a id="btnLogout"><i class="fa fa-sign-out fa-fw"></i> Logout</a>
                        </li>
                    </ul>
                </li>
            </ul>
            <!-- /.navbar-top-links -->
            <div class="sidebar" role="navigation">
                <div class="collapse navbar-collapse sidebar-nav">
                    <ul class="nav" id="side-menu">
                        <li class="sidebar-search">
                            <div class="input-group custom-search-form">
                                <input type="text" class="form-control" placeholder="Search...">
                                <span class="input-group-btn">
                                    <button class="btn btn-primary" type="button">
                                        <i class="fa fa-search"></i>
                                    </button>
                                </span>
                            </div>
                            <!-- /input-group -->
                        </li>
                        <li class="side-nav-links" id="dash" style="display:@Model.showDash">
                            <a class="active"><i class="fa fa-dashboard fa-fw"></i> Dashboard</a>
                        </li>
                        <li style="display:@Model.showOrg">
                            <a href="#"><i class="fa fa-building fa-fw"></i> Billers<span class="fa arrow"></span></a>
                            <ul class="nav nav-second-level collapse">
                                <li class="side-nav-links" id="new-org">
                                    <a>New</a>
                                </li>
                                <li class="side-nav-links" id="manage-org">
                                    <a>Manage</a>
                                </li>
                            </ul>
                            <!-- /.nav-second-level -->
                        </li>

                        <li style="display:@Model.showAdmin">
                            <a href="#"><i class="fa fa-user fa-fw"></i> Administrators<span class="fa arrow"></span></a>
                            <ul class="nav collapse nav-second-level">
                                <li class="side-nav-links" id="new-admin">
                                    <a>New</a>
                                </li>
                                <li class="side-nav-links" id="manage-admin">
                                    <a>Manage</a>
                                </li>
                            </ul>
                            <!-- /.nav-second-level -->
                        </li>
                        <li style="display:@Model.showClass">
                            <a href="#"><i class="fa fa-university fa-fw"></i> Classes<span class="fa arrow"></span></a>
                            <ul class="nav nav-second-level collapse">
                                <li class="side-nav-links" id="new-class">
                                    <a>New</a>
                                </li>
                                <li class="side-nav-links" id="manage-class">
                                    <a>Manage</a>
                                </li>
                            </ul>
                            <!-- /.nav-second-level -->
                        </li>
                        <li style="display:@Model.showTerm">
                            <a href="#"><i class="fa fa-mortar-board fa-fw"></i> Terms<span class="fa arrow"></span></a>
                            <ul class="nav nav-second-level collapse">
                                <li class="side-nav-links" id="new-term">
                                    <a>New</a>
                                </li>
                                <li class="side-nav-links" id="manage-term">
                                    <a>Manage</a>
                                </li>
                            </ul>
                            <!-- /.nav-second-level -->
                        </li>
                        <li style="display:@Model.showFees">
                            <a href="#"><i class="fa fa-money fa-fw"></i> Fees<span class="fa arrow"></span></a>
                            <ul class="nav nav-second-level collapse">
                                <li class="side-nav-links" id="new-fee">
                                    <a>New</a>
                                </li>
                                <li class="side-nav-links" id="manage-fee">
                                    <a>Manage</a>
                                </li>
                            </ul>
                            <!-- /.nav-second-level -->
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
        <div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header lbltag">DashBoard</h1>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
            <div id="div-dashboard" class="row">
                <div class="col-lg-3 col-md-6">
                    <div class="panel panel-primary">
                        <div class="panel-heading">
                            <div class="row">
                                <div class="col-xs-3">
                                    <i class="fa fa-building fa-5x"></i>
                                </div>
                                <div class="col-xs-9 text-right">
                                    <div class="huge"><label id="lblOrgCount"></label></div>
                                    <div>Billers</div>
                                </div>
                            </div>
                        </div>
                        <a href="#">
                            <div class="panel-footer">
                                <span class="pull-left">View Details</span>
                                <span class="pull-right"><i class="fa fa-arrow-circle-right"></i></span>
                                <div class="clearfix"></div>
                            </div>
                        </a>
                    </div>
                </div>
                <div class="col-lg-3 col-md-6">
                    <div class="panel panel-green">
                        <div class="panel-heading">
                            <div class="row">
                                <div class="col-xs-3">
                                    <i class="fa fa-user fa-5x"></i>
                                </div>
                                <div class="col-xs-9 text-right">
                                    <div class="huge"><label id="lblAdminCount"></label></div>
                                    <div>Administrators</div>
                                </div>
                            </div>
                        </div>
                        <a href="#">
                            <div class="panel-footer">
                                <span class="pull-left">View Details</span>
                                <span class="pull-right"><i class="fa fa-arrow-circle-right"></i></span>
                                <div class="clearfix"></div>
                            </div>
                        </a>
                    </div>
                </div>
                <div class="col-lg-3 col-md-6">
                    <div class="panel panel-yellow">
                        <div class="panel-heading">
                            <div class="row">
                                <div class="col-xs-3">
                                    <i class="fa fa-money fa-5x"></i>
                                </div>
                                <div class="col-xs-9 text-right">
                                    <div class="huge"><label id="lblFeeCount"></label></div>
                                    <div>Fees</div>
                                </div>
                            </div>
                        </div>
                        <a href="#">
                            <div class="panel-footer">
                                <span class="pull-left">View Details</span>
                                <span class="pull-right"><i class="fa fa-arrow-circle-right"></i></span>
                                <div class="clearfix"></div>
                            </div>
                        </a>
                    </div>
                </div>
                <div class="col-lg-3 col-md-6">
                    <div class="panel panel-red">
                        <div class="panel-heading">
                            <div class="row">
                                <div class="col-xs-3">
                                    <i class="fa fa-credit-card fa-5x"></i>
                                </div>
                                <div class="col-xs-9 text-right">
                                    <div class="huge"><label id="lblbpaymentCount"></label></div>
                                    <div>Payments</div>
                                </div>
                            </div>
                        </div>
                        <a href="#">
                            <div class="panel-footer">
                                <span class="pull-left">View Details</span>
                                <span class="pull-right"><i class="fa fa-arrow-circle-right"></i></span>
                                <div class="clearfix"></div>
                            </div>
                        </a>
                    </div>
                </div>
            </div>

            <div id="div-new-org" style="display:none;">
                <div id="div-org-search" class="row" style="height:60px; display:none;">
                    <div class="col-md-12">
                        <div class="form-group">
                            <div class="input-group custom-search-form">
                                <input id="txtSearchOrgs" type="text" class="form-control single-staff-name-tag" placeholder="Search billers...">
                                <span class="input-group-btn">
                                    <button id="btnSearchOrgs" class="btn btn-primary" type="button">
                                        <i class="fa fa-search"></i>
                                    </button>
                                </span>
                            </div>
                        </div>
                    </div>
                </div>
                <div id="div-org-form" class="panel panel-default" style="display:none;">
                    <div class="panel-heading">
                        <label id="label-panel-heading">Create a new biller</label>
                    </div>
                    <div class="panel-body">
                        <form role="form" id="form-new-org" action="" name="form-new-org">
                            @Html.AntiForgeryToken()
                            <fieldset>
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label for="ddlBillerCategory">Biller Category</label>
                                            <select id="ddlBillerCategory" class="form-control" name="ddlBillerCategory">
                                                <option value="0">Select Type</option>
                                            </select>
                                        </div>
                                    </div>                              
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label>Name</label>
                                            <input type="text" id="txtOrgName" class="form-control" name="txtOrgName" />
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label for="ddlBank">Bank</label>
                                            <select id="ddlBank" class="form-control" name="ddlBank">
                                                <option value="0">Select Bank</option>
                                            </select>
                                        </div>
                                    </div>                               
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label>Account Number</label>
                                            <input type="number" id="txtAcctNum" class="form-control" name="txtAcctNum" />
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label>Logo</label>
                                            <input type="file" id="fileOrgLogo" class="form-control" name="fileOrgLogo" accept=".jpg,.gif,.bmp,.png" />
                                            <a id="anchorFile" download></a>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label>Account Name</label>
                                            <input type="text" id="txtAcctName" class="form-control" name="txtAcctName" readonly />
                                        </div>
                                    </div>
                                </div>                             
                                <div class="row" style="height:80px;">
                                    <div class="col-md-12">
                                        <div class="form-group">
                                            <label>Street</label>
                                            <input type="text" id="txtOrgAddress" class="form-control" name="txtOrgAddress" />
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label>City</label>
                                            <input type="text" id="txtOrgCity" class="form-control" name="txtOrgCity" />
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label>Post Code</label>
                                            <input type="text" id="txtOrgPostCode" class="form-control" name="txtOrgPostCode" />
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label>Country</label>
                                            <select id="ddlCountry" class="form-control" name="ddlCountry">
                                                <option value="0">Select Country</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label>State</label>
                                            <select id="ddlStates" class="form-control" name="ddlStates">
                                                <option value="0">Select State</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label>Email</label>
                                            <input type="email" id="txtOrgEmail" class="form-control" name="txtOrgEmail" />
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label>Phone</label>
                                            <input type="number" id="txtOrgPhone" class="form-control" name="txtOrgPhone" />
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label>Email (Alternate)</label>
                                            <input type="email" id="txtOrgAltEmail" class="form-control" name="txtOrgAltEmail" />
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label>Phone (Alternate)</label>
                                            <input type="number" id="txtOrgAltPhone" class="form-control" name="txtOrgAltPhone" />
                                        </div>
                                    </div>
                                </div>
                                <div class="row" style="height:80px;">
                                    <div class="col-md-12">
                                        <div class="form-group text-right">
                                            <button type="submit" id="btnSubmitOrg" class="btn btn-primary">Submit</button>
                                        </div>
                                    </div>
                                </div>
                            </fieldset>
                        </form>
                    </div>
                </div>
            </div>


            <div id="div-manage-org" style="display:none;">

            </div>


            <div id="div-new-admin" style="display:none;">
                <div id="div-admin-search" class="row" style="height:60px; display:none;">
                    <div class="col-md-12">
                        <div class="form-group">
                            <div class="input-group custom-search-form">
                                <input id="txtSearchAdmin" type="text" class="form-control single-admin-name-tag searchAdmin" placeholder="Search Administrators...">
                                <span class="input-group-btn">
                                    <button id="btnSearchAdmin" class="btn btn-primary" type="button">
                                        <i class="fa fa-search"></i>
                                    </button>
                                </span>
                            </div>
                        </div>
